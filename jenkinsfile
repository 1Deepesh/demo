node {
    stage ('SCM') {
        echo 'Gathering code from github ....'
        git branch: '$branch'; url: "https://github.com/1Deepesh/demo.git"
    }
    stage ('build') {
        try{echo 'Testing ....'
            echo 'Hello world'
            sh 'date'
            sh 'uptime'
            sh 'dateljkdf'
        }catch(ex) {
            echo "sorry something went wrong."
        }
        finally {
            echo "finally block executed."
        }
    }
    stage ('Deploy') {
        echo 'deploy ...'
    }
}
